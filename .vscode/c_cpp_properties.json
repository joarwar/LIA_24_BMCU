{
    "configurations": [
        {
            "name": "Win32",
            "includePath": [
                "${workspaceRoot}",
                "${workspaceRoot}/Core/Inc",
                "${workspaceRoot}/Drivers/STM32WBAxx_HAL_Driver/Inc",
                "${workspaceRoot}/Drivers/CMSIS/Include",
                "${workspaceRoot}/Drivers/CMSIS/Device/ST/STM32WBAxx/Include",
                "${workspaceRoot}/Drivers/STM32WBAxx_HAL_Driver/Src",
                "${workspaceFolder}/Core/Src",
                "${workspaceFolder}/Drivers/BSP/STM32WBAxx_Nucleo",
                "/dev/tools/gcc-arm-none-eabi-13.3/arm-none-eabi/",
                "/dev/tools/gcc-arm-none-eabi-13.3/arm-none-eabi/include",
                "/dev/tools/gcc-arm-none-eabi-13.3/arm-none-eabi/include/machine",
                "/dev/tools/gcc-arm-none-eabi-13.3/arm-none-eabi/include/newlib-nano",
                "/dev/tools/gcc-arm-none-eabi-13.3/arm-none-eabi/include/sys"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE",
                "USE_HAL_DRIVER",
                "STM32WBAxx"
            ],
            "intelliSenseMode": "gcc-arm",
            "compilerPath": "${env:ARMGCC_DIR}/arm-none-eabi-gcc",
            "compilerArgs": [
                "-mcpu=cortex-m33",
                "-mfpu=fpv4-sp-d16", 
                "-mfloat-abi=hard",
                "-mthumb"
            ],
            "cStandard": "c17",
            "cppStandard": "c++17",
            "configurationProvider": "ms-vscode.makefile-tools"
        }
    ],
    "version": 4
}
